var shrinkFlag_applyConstractPayNotice = 0;

	function shrinkFrame(frameName){
		var iFrame;
		if(frameName == "accountWorkList"){
			iFrame = document.getElementById("AccountWorkList");
			if(shrinkFlag_applyConstractPayNotice == 0){
				iFrame.style.display = "none";
				shrinkFlag_applyConstractPayNotice = 1;
			}else{
				iFrame.style.display = "block";
				shrinkFlag_applyConstractPayNotice = 0;
			}			
		}
		if(frameName == "WorkList"){
			iFrame = document.getElementById("workList");
			if(shrinkFlag_applyConstractPayNotice == 0){
				iFrame.style.display = "none";
				shrinkFlag_applyConstractPayNotice = 1;
			}else{
				iFrame.style.display = "block";
				shrinkFlag_applyConstractPayNotice = 0;
			}			
		}
	}