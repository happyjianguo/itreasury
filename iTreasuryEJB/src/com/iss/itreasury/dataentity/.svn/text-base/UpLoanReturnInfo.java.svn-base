public class UpLoanReturnInfo implements java.io.Serializable
{
   	private boolean IsOk = false;
    private String Reason  = ""; // 失败原因,每一条原因都有一个发生位置，在
    private long PositionRow = -1 ; // 失败的记录的行位置
    private int PositionCol = -1 ; // 失败的记录的列位置

    /**
     * @return
     */
    public boolean getIsOk()
    {
        return IsOk;
    }

    /**
     * @return
     */
    public int getPositionCol()
    {
        return PositionCol;
    }

    /**
     * @return
     */
    public long getPositionRow()
    {
        return PositionRow;
    }

    /**
     * @return
     */
    public String getReason()
    {
        return Reason;
    }

    /**
     * @param b
     */
    public void setIsOk(boolean b)
    {
        IsOk = b;
    }

    /**
     * @param i
     */
    public void setPositionCol(int i)
    {
        PositionCol = i;
    }

    /**
     * @param l
     */
    public void setPositionRow(long l)
    {
        PositionRow = l;
    }

    /**
     * @param string
     */
    public void setReason(String string)
    {
        Reason = string;
    }
}
