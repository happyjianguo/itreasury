    public long statusid = -1;              //状态
    public String remarks = "";             //备注
    public long officeid = -1;              
    public long currencyid = -1;
    public long inputuserid = -1;
    public Timestamp inputdate = null;
    
    
	public long getId() {
		return id;
	}
	public void setId(long id) {
		putUsedField("id", id);
		this.id = id;
	}
	public String getCode() {
		return code;
	}
