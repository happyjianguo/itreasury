package com.iss.itreasury.system.setting.dataentity ;

import java.io.Serializable;

import com.iss.itreasury.securities.util.SECBaseDataEntity;



public class DatabaseConnectionSetInfo extends SECBaseDataEntity implements Serializable
{
	private long		Id						= -1 ;     
	private long		officeID				= -1 ;     //办事处
	private long 		currencyID			    = -1 ;     //币种
	private String		databaseName			= "" ;     //数据库名称
	private String		databaseIP		        = "" ;     //数据库IP
	private String		databasePort	        = "" ;	   //数据库端口	
	private String		databaseUser			        = "" ;     //用户
	private String		databasePassword		        = "" ;     //密码
	private String		setNo       	        = "" ;	   //账套编号
	private long		statusID			    = -1 ;     //状态
	/**
	 * @return Returns the currencyID.
	 */
	public long getCurrencyID() {
		return currencyID;
	}
	/**
	 * @param currencyID The currencyID to set.
	 */
	public void setCurrencyID(long currencyID) {
		this.currencyID = currencyID;
		putUsedField("currencyID", currencyID);
	}
	/**
	 * @return Returns the databaseIP.
	 */
	public String getDatabaseIP() {
		return databaseIP;
	}
	/**
	 * @param databaseIP The databaseIP to set.
	 */
	public void setDatabaseIP(String databaseIP) {
		this.databaseIP = databaseIP;
		putUsedField("databaseIP", databaseIP);
	}
	/**
	 * @return Returns the databaseName.
	 */
	public String getDatabaseName() {
		return databaseName;
	}
	/**
	 * @param databaseName The databaseName to set.
	 */
	public void setDatabaseName(String databaseName) {
		this.databaseName = databaseName;
		putUsedField("databaseName", databaseName);
	}
	/**
	 * @return Returns the databasePort.
	 */
	public String getDatabasePort() {
		return databasePort;
	}
	/**
	 * @param databasePort The databasePort to set.
	 */
	public void setDatabasePort(String databasePort) {
		this.databasePort = databasePort;
		putUsedField("databasePort", databasePort);
	}
	/**
	 * @return Returns the id.
	 */
	public long getId() {
		return Id;
	}
	/**
	 * @param id The id to set.
	 */
	public void setId(long id) {
		Id = id;
		putUsedField("id", id);
	}
	/**
	 * @return Returns the officeID.
	 */
	public long getOfficeID() {
		return officeID;
	}
	/**
	 * @param officeID The officeID to set.
	 */
	public void setOfficeID(long officeID) {
		this.officeID = officeID;
		putUsedField("officeID", officeID);
	}
	/**
	 * @return Returns the password.
	 */
	public String getDatabasePassword() {
		return databasePassword;
	}
	/**
	 * @param password The password to set.
	 */
	public void setDatabasePassword(String password) {
		this.databasePassword = password;
		putUsedField("databasePassword", password);
	}
	/**
	 * @return Returns the setNo.
	 */
	public String getSetNo() {
		return setNo;
	}
	/**
	 * @param setNo The setNo to set.
	 */
	public void setSetNo(String setNo) {
		this.setNo = setNo;
		putUsedField("setNo", setNo);
	}
