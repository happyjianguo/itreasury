package com.iss.itreasury.itreasuryinfo.branch.dataentity;

import java.sql.Timestamp;

public class BranchRateQueryResultInfo {
	private long branchID = -1;
	private String bankAccountCode = "";
	private String bankAccountName = "";
	private long bankTypeID = -1;
	private Timestamp validate = null;
	private double rate = 0.0;
	public long getBranchID() {
		return branchID;
	}
	public void setBranchID(long branchID) {
		this.branchID = branchID;
	}
	public String getBankAccountCode() {
		return bankAccountCode;
	}
	public void setBankAccountCode(String bankAccountCode) {
		this.bankAccountCode = bankAccountCode;
	}
	public String getBankAccountName() {
		return bankAccountName;
	}
	public void setBankAccountName(String bankAccountName) {
		this.bankAccountName = bankAccountName;
	}
	public long getBankTypeID() {
		return bankTypeID;
	}
	public void setBankTypeID(long bankTypeID) {
		this.bankTypeID = bankTypeID;
	}
	public Timestamp getValidate() {
		return validate;
	}
	public void setValidate(Timestamp validate) {
		this.validate = validate;
	}
	public double getRate() {
		return rate;
	}
	public void setRate(double rate) {
		this.rate = rate;
	}
	
	
}
