package com.iss.itreasury.report.dateentity;

import java.sql.Timestamp;

public class SettLoanInterestInfo {
	
	private String strTransNo = null;		//交易记录查询
	
	private long lContractId = -1;			//贷款合同ID
	
	private long lTransTypeId = -1;			//交易类型ID
	
	private long lLoanAccountId = -1;		//贷款账户ID
	
	private long lClientId = -1;			//贷款客户ID
	
	private long lReceiveAccountId = -1;	//收息账户ID
	
	private double dbInterest = 0.00;		//利息
	
	private Timestamp tsExecuteDate = null;	//执行日
	
	private Timestamp tsInterestDate = null;//结息日

	public SettLoanInterestInfo() {
		
	}

	public double getDbInterest() {
		return dbInterest;
	}

	public void setDbInterest(double dbInterest) {
		this.dbInterest = dbInterest;
	}

	public long getLClientId() {
		return lClientId;
	}

	public void setLClientId(long clientId) {
		lClientId = clientId;
	}

	public long getLContractId() {
		return lContractId;
	}

	public void setLContractId(long contractId) {
		lContractId = contractId;
	}

	public long getLLoanAccountId() {
		return lLoanAccountId;
	}

	public void setLLoanAccountId(long loanAccountId) {
		lLoanAccountId = loanAccountId;
	}

	public long getLReceiveAccountId() {
		return lReceiveAccountId;
	}

	public void setLReceiveAccountId(long receiveAccountId) {
		lReceiveAccountId = receiveAccountId;
	}

	public long getLTransTypeId() {
		return lTransTypeId;
	}

	public void setLTransTypeId(long transTypeId) {
		lTransTypeId = transTypeId;
	}

	public String getStrTransNo() {
		return strTransNo;
	}

	public void setStrTransNo(String strTransNo) {
		this.strTransNo = strTransNo;
	}

	public Timestamp getTsExecuteDate() {
		return tsExecuteDate;
	}

	public void setTsExecuteDate(Timestamp tsExecuteDate) {
		this.tsExecuteDate = tsExecuteDate;
	}

	public Timestamp getTsInterestDate() {
		return tsInterestDate;
	}

	public void setTsInterestDate(Timestamp tsInterestDate) {
		this.tsInterestDate = tsInterestDate;
	}
	
	
}
