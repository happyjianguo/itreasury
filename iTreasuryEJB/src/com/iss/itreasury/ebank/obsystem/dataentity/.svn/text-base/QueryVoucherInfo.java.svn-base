public class QueryVoucherInfo implements java.io.Serializable
{


    // 可选条件
	private long lTypeID = -1; // 票据类型
    private String sStartDate = null; // 申请日期-从
    private String sEndDate = null; // 申请日期-到
    private long lStatus = -1; // 票据状态
    private String sStartVoucherNo = ""; // 票据号-从
    private String sEndVoucherNo = ""; // 票据号-到
    

    // 必选条件
    private long lClientID = -1; // 客户ID
    private long lCurrencyID = -1; //币种ID

    // 排序和分页
    private long lTotalPages = -1;
    private long lPageNo = -1;
    private long lLinesOfPage = -1;

	

	/**
	 * @return
	 */
	public long getClientID() {
		return lClientID;
	}

	/**
	 * @return
	 */
	public long getCurrencyID() {
		return lCurrencyID;
	}

	/**
	 * @return
	 */
	public String getEndVoucherNo() {
		return sEndVoucherNo;
	}

	/**
	 * @return
	 */
	public long getLinesOfPage() {
		return lLinesOfPage;
	}

	/**
	 * @return
	 */
	public long getPageNo() {
		return lPageNo;
	}

	/**
	 * @return
	 */
	public String getStartVoucherNo() {
		return sStartVoucherNo;
	}

	/**
	 * @return
	 */
	public long getStatus() {
		return lStatus;
	}

	/**
	 * @return
	 */
	public long getTotalPages() {
		return lTotalPages;
	}

	/**
	 * @return
	 */
	public String getEndDate() {
		return sEndDate;
	}

	/**
	 * @return
	 */
	public String getStartDate() {
		return sStartDate;
	}

	/**
	 * @param l
	 */
	public void setClientID(long l) {
		lClientID = l;
	}

	/**
	 * @param l
	 */
	public void setCurrencyID(long l) {
		lCurrencyID = l;
	}

	/**
	 * @param l
	 */
	public void setEndVoucherNo(String string) {
		sEndVoucherNo = string;
	}

	/**
	 * @param l
	 */
	public void setLinesOfPage(long l) {
		lLinesOfPage = l;
	}

	/**
	 * @param l
	 */
	public void setPageNo(long l) {
		lPageNo = l;
	}

	/**
	 * @param l
	 */
	public void setStartVoucherNo(String string) {
		sStartVoucherNo = string;
	}

	/**
	 * @param l
	 */
	public void setStatus(long l) {
		lStatus = l;
	}

	/**
	 * @param l
	 */
	public void setTotalPages(long l) {
		lTotalPages = l;
	}

	/**
	 * @param string
	 */
	public void setEndDate(String string) {
		sEndDate = string;
	}

	/**
	 * @param string
	 */
	public void setStartDate(String string) {
		sStartDate = string;
	}

	/**
	 * @return
	 */
	public long getTypeID() {
		return lTypeID;
	}

	/**
	 * @param l
	 */
	public void setTypeID(long l) {
		lTypeID = l;
	}

}
