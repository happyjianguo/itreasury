package com.iss.itreasury.loan.assureloan.dataentity;


public class AssureTempInfo {
	private String contractNo;
	private long bankCreditId;
	private double amount;
	private String companyCode;
	private String variety;
	
	public double getAmount() {
		return amount;
	}
	public void setAmount(double amount) {
		this.amount = amount;
	}
	public long getBankCreditId() {
		return bankCreditId;
	}
	public void setBankCreditId(long bankCreditId) {
		this.bankCreditId = bankCreditId;
	}
	public String getCompanyCode() {
		return companyCode;
	}
	public void setCompanyCode(String companyCode) {
		this.companyCode = companyCode;
	}
	public String getContractNo() {
		return contractNo;
	}
	public void setContractNo(String contractNo) {
		this.contractNo = contractNo;
	}
	public String getVariety() {
		return variety;
	}
	public void setVariety(String variety) {
		this.variety = variety;
	}
}
