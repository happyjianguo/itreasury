package com.iss.itreasury.loan.integratedCredit.customerfeedback.dataentity;

/**
 * 行业指标Bean
 * @author lcliu
 */
public class FinancialTargetInfo
{
	private long clientID = 0;		// 客户ID
	private long callingType = 0;	// 行业类型
	private double originalData = 0;	// 原始数据
	private double newData = 0;		// 指标数据

	public long getCallingType() {
		return callingType;
	}
	public void setCallingType(long callingType) {
		this.callingType = callingType;
	}
	public long getClientID() {
		return clientID;
	}
	public void setClientID(long clientID) {
		this.clientID = clientID;
	}
	public double getNewData() {
		return newData;
	}
	public void setNewData(double newData) {
		this.newData = newData;
	}
	public double getOriginalData() {
		return originalData;
	}
	public void setOriginalData(double originalData) {
		this.originalData = originalData;
	}
}
