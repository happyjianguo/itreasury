package com.iss.itreasury.loan.repurchase.dataentity;

import java.sql.Timestamp;

import com.iss.itreasury.loan.base.LoanBaseDataEntity;

/**
 * @author shantao
 * 
 * @version 1.0
 * 2005-12-27
 */
public class RepurchaseItemInfo extends LoanBaseDataEntity
{
    private long repurchaseID;
    private long contractID;
//    private long nBorrowClientID;
    private String nBorrowClientName;
    private String sContractCode = "";
    private long nTypeID;
    private String typeName;
    private Timestamp dtStartDate;
    private Timestamp dtEndDate;
    private double mLoanAmount;
    private double balanceAmount;
    /**
     * @return Returns the contractID.
     */
    public long getContractID()
    {
        return contractID;
        
    }
    /**
     * @param contractID The contractID to set.
     */
    public void setContractID(long contractID)
    {
        this.contractID = contractID;
        this.putUsedField("contractID",contractID);
    }
    /**
     * @return Returns the dtEndDate.
     */
    public Timestamp getDtEndDate()
    {
        return dtEndDate;
    }
    /**
     * @param dtEndDate The dtEndDate to set.
     */
    public void setDtEndDate(Timestamp dtEndDate)
    {
        this.dtEndDate = dtEndDate;
    }
    /**
     * @return Returns the dtStartDate.
     */
    public Timestamp getDtStartDate()
    {
        return dtStartDate;
    }
    /**
     * @param dtStartDate The dtStartDate to set.
     */
    public void setDtStartDate(Timestamp dtStartDate)
    {
        this.dtStartDate = dtStartDate;
    }
    /**
     * @return Returns the mLoanAmount.
     */
    public double getMLoanAmount()
    {
        return mLoanAmount;
    }
    /**
     * @param loanAmount The mLoanAmount to set.
     */
    public void setMLoanAmount(double loanAmount)
    {
        mLoanAmount = loanAmount;
    }

    /**
     * @return Returns the nBorrowClientName.
     */
    public String getNBorrowClientName()
    {
        return nBorrowClientName;
    }

    /**
     * @param borrowClientName
     *            The nBorrowClientName to set.
     */
    public void setNBorrowClientName(String borrowClientName)
    {
        nBorrowClientName = borrowClientName;
    }
    /**
     * @return Returns the nTypeID.
     */
    public long getNTypeID()
    {
        return nTypeID;
    }
    /**
     * @param typeID The nTypeID to set.
     */
    public void setNTypeID(long typeID)
    {
        nTypeID = typeID;
    }
    /**
     * @return Returns the repurchaseID.
     */
    public long getRepurchaseID()
    {
        return repurchaseID;
    }
    /**
     * @param repurchaseID The repurchaseID to set.
     */
    public void setRepurchaseID(long repurchaseID)
    {
        this.repurchaseID = repurchaseID;
        this.putUsedField("repurchaseID",repurchaseID);
    }
    /**
     * @return Returns the sContractCode.
     */
    public String getSContractCode()
    {
        return sContractCode;
    }
    /**
     * @param contractCode The sContractCode to set.
     */
    public void setSContractCode(String contractCode)
    {
        sContractCode = contractCode;
    }
    
    /**
     * @return Returns the balanceAmount.
     */
    public double getBalanceAmount()
    {
        return balanceAmount;
    }
    /**
     * @param balanceAmount The balanceAmount to set.
     */
    public void setBalanceAmount(double balanceAmount)
    {
        this.balanceAmount = balanceAmount;
    }
    /**
     * @return Returns the typeName.
     */
    public String getTypeName()
    {
        return typeName;
    }
    /**
     * @param typeName The typeName to set.
     */
    public void setTypeName(String typeName)
    {
        this.typeName = typeName;
    }
}
