public class InterestDetailInfo implements java.io.Serializable{
	
	private long id = -1; 
	private double rate = 0.0;//利率
	private long bankLoanID = -1;//银行台账ID
	private String startDate = "";//起始日期
	public long getBankLoanID() {
		return bankLoanID;
	}
	public void setBankLoanID(long bankLoanID) {
		this.bankLoanID = bankLoanID;
	}
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public double getRate() {
		return rate;
	}
	public void setRate(double rate) {
		this.rate = rate;
	}
	public String getStartDate() {
		return startDate;
	}
	public void setStartDate(String startDate) {
		this.startDate = startDate;
	}
	
}
