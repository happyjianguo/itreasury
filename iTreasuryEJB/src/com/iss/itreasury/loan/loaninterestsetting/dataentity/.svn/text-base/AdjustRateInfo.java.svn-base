package com.iss.itreasury.loan.loaninterestsetting.dataentity;
import java.sql.Timestamp;

public class AdjustRateInfo {
	private String sContractCode = "";
	private String sPayformCode = "";
	private long lPayformTerm = -1;
	private double dRate = 0.0;
	private double dInterestRate = 0.0;
	private double dNewRate = 0.0;
	private Timestamp dtValiDate = null;
	private Timestamp dtRateAdjustDate = null;
	
	public String getSContractCode() {
		return sContractCode;
	}
	public void setSContractCode(String contractCode) {
		sContractCode = contractCode;
	}
	public String getSPayformCode() {
		return sPayformCode;
	}
	public void setSPayformCode(String payformCode) {
		sPayformCode = payformCode;
	}
	public long getLPayformTerm() {
		return lPayformTerm;
	}
	public void setLPayformTerm(long payformTerm) {
		lPayformTerm = payformTerm;
	}
	public double getDRate() {
		return dRate;
	}
	public void setDRate(double rate) {
		dRate = rate;
	}
	public double getDInterestRate() {
		return dInterestRate;
	}
	public void setDInterestRate(double interestRate) {
		dInterestRate = interestRate;
	}
	public double getDNewRate() {
		return dNewRate;
	}
	public void setDNewRate(double newRate) {
		dNewRate = newRate;
	}
	public Timestamp getDtValiDate() {
		return dtValiDate;
	}
	public void setDtValiDate(Timestamp dtValiDate) {
		this.dtValiDate = dtValiDate;
	}
	public Timestamp getDtRateAdjustDate() {
		return dtRateAdjustDate;
	}
	public void setDtRateAdjustDate(Timestamp dtRateAdjustDate) {
		this.dtRateAdjustDate = dtRateAdjustDate;
	}
	
}