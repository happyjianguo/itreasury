package com.iss.itreasury.loan.loaninterestsetting.dataentity;

import java.sql.Timestamp;

import com.iss.itreasury.system.approval.dataentity.InutParameterInfo;

public class AdjustPayConditionInfo implements java.io.Serializable{

	public long id;
	public long nbankinterestid;//银行利率id
	public String ncontractid;//合同的id
	public double mrate;//利率
	public Timestamp dtvalidate;//生效时间
	public String sreason;//原因
	public long ninputuserid;//录入人
	public Timestamp dtinputdate;//录入时间
	public long nnextcheckuserid;//审核人
	public long nstatusid;//状态
	public long nofficeid;//办事处
	public long nloanpaynoticeid;//放款通知单id
	public long nnextchecklevel;//下一个审批级别
	public double mstaidadjustrate;//固定浮动利率
	public double madjustrate;//浮动比例
	public long ncurrencyid;//币种
	public long nPlanID= -1;//计划调整ID(融资租赁使用)
	public long batchID = -1;//新增批次号id
	public InutParameterInfo inutParameterInfo = null;
	
	//合同计划修改版本ID
	public long lModifyID = -1;//修改版本ID
	
	public AdjustPayConditionInfo()
	{
		super();
	}
	
	public Timestamp getDtinputdate() {
		return dtinputdate;
	}
	public void setDtinputdate(Timestamp dtinputdate) {
		this.dtinputdate = dtinputdate;
	}
	public Timestamp getDtvalidate() {
		return dtvalidate;
	}
	public void setDtvalidate(Timestamp dtvalidate) {
		this.dtvalidate = dtvalidate;
	}
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public InutParameterInfo getInutParameterInfo() {
		return inutParameterInfo;
	}
	public void setInutParameterInfo(InutParameterInfo inutParameterInfo) {
		this.inutParameterInfo = inutParameterInfo;
	}
	public double getMrate() {
		return mrate;
	}

	public long getNbankinterestid() {
		return nbankinterestid;
	}
	public void setNbankinterestid(long nbankinterestid) {
		this.nbankinterestid = nbankinterestid;
	}
	public String getNcontractid() {
		return ncontractid;
	}
	public void setNcontractid(String ncontractid) {
		this.ncontractid = ncontractid;
	}
	public long getNcurrencyid() {
		return ncurrencyid;
	}
	public void setNcurrencyid(long ncurrencyid) {
		this.ncurrencyid = ncurrencyid;
	}
	public long getNinputuserid() {
		return ninputuserid;
	}
	public void setNinputuserid(long ninputuserid) {
		this.ninputuserid = ninputuserid;
	}
	public long getNloanpaynoticeid() {
		return nloanpaynoticeid;
	}
	public void setNloanpaynoticeid(long nloanpaynoticeid) {
		this.nloanpaynoticeid = nloanpaynoticeid;
	}
	public long getNnextchecklevel() {
		return nnextchecklevel;
	}
	public void setNnextchecklevel(long nnextchecklevel) {
		this.nnextchecklevel = nnextchecklevel;
	}
	public long getNnextcheckuserid() {
		return nnextcheckuserid;
	}
	public void setNnextcheckuserid(long nnextcheckuserid) {
		this.nnextcheckuserid = nnextcheckuserid;
	}
	public long getNofficeid() {
		return nofficeid;
	}
	public void setNofficeid(long nofficeid) {
		this.nofficeid = nofficeid;
	}
	public long getNstatusid() {
		return nstatusid;
	}
	public void setNstatusid(long nstatusid) {
		this.nstatusid = nstatusid;
	}
	public String getSreason() {
		return sreason;
	}
	public void setSreason(String sreason) {
		this.sreason = sreason;
	}

	public void setMadjustrate(double madjustrate) {
		this.madjustrate = madjustrate;
	}

	public void setMstaidadjustrate(double mstaidadjustrate) {
		this.mstaidadjustrate = mstaidadjustrate;
	}

	public double getMadjustrate() {
		return madjustrate;
	}

	public double getMstaidadjustrate() {
		return mstaidadjustrate;
	}

	public void setMrate(double mrate) {
		this.mrate = mrate;
	}

	public long getLModifyID() {
		return lModifyID;
	}

	public void setLModifyID(long modifyID) {
		lModifyID = modifyID;
	}

	public long getNPlanID() {
		return nPlanID;
	}

	public void setNPlanID(long planID) {
		nPlanID = planID;
	}

	public long getBatchID() {
		return batchID;
	}

	public void setBatchID(long batchID) {
		this.batchID = batchID;
	}
	
	
}
