package com.iss.itreasury.loan.lettercredit.dataentity;



public class LetterCreditPaymentInfo {
	private int id;
	private String innerCode;		//内部流水号
	private String paymentDate;		//实际付款日期
	private double paymentAmout;	//实际付款金额
	
	public double getPaymentAmout() {
		return paymentAmout;
	}

	public void setPaymentAmout(double paymentAmout) {
		this.paymentAmout = paymentAmout;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getInnerCode() {
		return this.innerCode;
	}
	
	public void setInnerCode(String innerCode) {
		this.innerCode = innerCode;
	}
	
	public String getPaymentDate() {
		return this.paymentDate;
	}
	
	public void setPaymentDate(String paymentDate) {
		this.paymentDate = paymentDate;
	}
	
}
