package com.iss.itreasury.loan.aftercredit.dataentity;

import java.util.List;

public class ClientLoanInfo 
{
	String clientName = "";						//客户名称
	String clientCode = "";						//客户编号
	String creditLevel = "";					//客户信用等级
	long alarmConclution = -1;					//预警状态
	List contractList = null;					//合同List
	public long getAlarmConclution() {
		return alarmConclution;
	}
	public void setAlarmConclution(long alarmConclution) {
		this.alarmConclution = alarmConclution;
	}
	public String getClientCode() {
		return clientCode;
	}
	public void setClientCode(String clientCode) {
		this.clientCode = clientCode;
	}
	public String getClientName() {
		return clientName;
	}
	public void setClientName(String clientName) {
		this.clientName = clientName;
	}
	public List getContractList() {
		return contractList;
	}
	public void setContractList(List contractList) {
		this.contractList = contractList;
	}
	public String getCreditLevel() {
		return creditLevel;
	}
	public void setCreditLevel(String creditLevel) {
		this.creditLevel = creditLevel;
	}
}
