package com.iss.itreasury.loan.loanletterguarantee.dataentity;

public class GuaranteeChargeInfo implements java.io.Serializable{
	
	private long id = -1; 
	private double nRate = 0.0;    //费率
	private long guaranteeID = -1; //对应保函ID
	private long nSection = -1 ;   //区间 1-季 2-年
	private String dPaymentDate = "";    //付款日期
	private double mPaymentAmout = 0.0; //付款金额
	
	public String getDPaymentDate() {
		return dPaymentDate;
	}
	public void setDPaymentDate(String paymentDate) {
		dPaymentDate = paymentDate;
	}
	public long getGuaranteeID() {
		return guaranteeID;
	}
	public void setGuaranteeID(long guaranteeID) {
		this.guaranteeID = guaranteeID;
	}
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public double getMPaymentAmout() {
		return mPaymentAmout;
	}
	public void setMPaymentAmout(double paymentAmout) {
		mPaymentAmout = paymentAmout;
	}
	public double getNRate() {
		return nRate;
	}
	public void setNRate(double rate) {
		nRate = rate;
	}
	public long getNSection() {
		return nSection;
	}
	public void setNSection(long section) {
		nSection = section;
	}
	
}

