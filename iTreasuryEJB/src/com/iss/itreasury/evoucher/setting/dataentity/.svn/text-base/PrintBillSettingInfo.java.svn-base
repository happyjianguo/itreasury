package com.iss.itreasury.evoucher.setting.dataentity;

public class PrintBillSettingInfo {

	private long id = -1;
	private long lCurrencyID = -1;//币种
	private long lOfficeID = -1;//办事处
	private long lModuleID = -1;//模块id
	private long lBillsegment = -1;//单据联数
	private long lPrintType = -1;//打印类型
	private String sName = "";//单据名称
	private long nStatusID = -1;//状态
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public long getLBillsegment() {
		return lBillsegment;
	}
	public void setLBillsegment(long billsegment) {
		lBillsegment = billsegment;
	}
	public long getLCurrencyID() {
		return lCurrencyID;
	}
	public void setLCurrencyID(long currencyID) {
		lCurrencyID = currencyID;
	}
	public long getLModuleID() {
		return lModuleID;
	}
	public void setLModuleID(long moduleID) {
		lModuleID = moduleID;
	}
	public long getLOfficeID() {
		return lOfficeID;
	}
	public void setLOfficeID(long officeID) {
		lOfficeID = officeID;
	}
	public long getLPrintType() {
		return lPrintType;
	}
	public void setLPrintType(long printType) {
		lPrintType = printType;
	}
	public long getNStatusID() {
		return nStatusID;
	}
	public void setNStatusID(long statusID) {
		nStatusID = statusID;
	}
	public String getSName() {
		return sName;
	}
	public void setSName(String name) {
		sName = name;
	}
	
	
}
