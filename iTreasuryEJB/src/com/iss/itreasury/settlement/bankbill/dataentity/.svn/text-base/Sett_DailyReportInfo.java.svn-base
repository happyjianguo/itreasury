		IncomeCount = l;
	}

	/**
	 * @param l
	 */
	public void setOriginalCount(long l)
	{
		OriginalCount = l;
	}

	/**
	 * @param l
	 */
	public void setOutCount(long l)
	{
		OutCount = l;
	}

}
