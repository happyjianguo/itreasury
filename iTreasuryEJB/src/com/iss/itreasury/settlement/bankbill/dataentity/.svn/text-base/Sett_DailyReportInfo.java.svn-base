package com.iss.itreasury.settlement.bankbill.dataentity;

import java.sql.*;
import java.io.Serializable;

public class Sett_DailyReportInfo implements Serializable
{
	private long ID = -1; //唯一标识
	private Timestamp Date = null;//日期
	private long BilliTypeID = -1; //票据类型ID
	private long OriginalCount = -1; //
	private long IncomeCount = -1; //
	private long OutCount = -1; //
	private long Balance = -1; //
	/**
	 * @return
	 */
	public long getBalance()
	{
		return Balance;
	}

	/**
	 * @return
	 */
	public long getBilliTypeID()
	{
		return BilliTypeID;
	}

	/**
	 * @return
	 */
	public Timestamp getDate()
	{
		return Date;
	}

	/**
	 * @return
	 */
	public long getID()
	{
		return ID;
	}

	/**
	 * @return
	 */
	public long getIncomeCount()
	{
		return IncomeCount;
	}

	/**
	 * @return
	 */
	public long getOriginalCount()
	{
		return OriginalCount;
	}

	/**
	 * @return
	 */
	public long getOutCount()
	{
		return OutCount;
	}

	/**
	 * @param l
	 */
	public void setBalance(long l)
	{
		Balance = l;
	}

	/**
	 * @param l
	 */
	public void setBilliTypeID(long l)
	{
		BilliTypeID = l;
	}

