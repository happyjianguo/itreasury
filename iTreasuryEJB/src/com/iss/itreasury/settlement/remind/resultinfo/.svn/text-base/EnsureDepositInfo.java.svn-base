package com.iss.itreasury.settlement.remind.resultinfo;

import java.sql.Timestamp;

/**
 * @author zyzhu
 * 
 */
public class EnsureDepositInfo implements java.io.Serializable {

	// 定义成员变量
	private String sDepositNo = null;	//保证金存单号

	private long nAccountId = -1;	//账号ID

	private long nClientId = -1;	//客户ID

	private Timestamp dtEnd = null;	//到期日期

	/**
	 * 到期日期
	 * @return 返回 dtEnd。
	 */
	public Timestamp getDtEnd() {
		return dtEnd;
	}

	/**
	 * 到期日期
	 * @param dtEnd 要设置的 dtEnd。
	 */
	public void setDtEnd(Timestamp dtEnd) {
		this.dtEnd = dtEnd;
	}

	/**
	 * 账号ID
	 * @return 返回 nAccountId。
	 */
	public long getNAccountId() {
		return nAccountId;
	}

	/**
	 * 账号ID
	 * @param accountId 要设置的 nAccountId。
	 */
	public void setNAccountId(long accountId) {
		nAccountId = accountId;
	}

	/**
	 * 客户ID
	 * @return 返回 nClientId。
	 */
	public long getNClientId() {
		return nClientId;
	}

	/**
	 * 客户ID
	 * @param clientId 要设置的 nClientId。
	 */
	public void setNClientId(long clientId) {
		nClientId = clientId;
	}

	/**
	 * 保证金存单号
	 * @return 返回 sDepositNo。
	 */
	public String getSDepositNo() {
		return sDepositNo;
	}

	/**
	 * 保证金存单号
	 * @param depositNo 要设置的 sDepositNo。
	 */
	public void setSDepositNo(String depositNo) {
		sDepositNo = depositNo;
	}

	
}
