public class CurrencySubjectInfo implements Serializable {

	private String tableName = "";
	private long recordID = -1;
	private long currencyID = -1;
	private String subject = "";
	private long backOfficeID = -1; 
	private String interestSubject = "";
	private String bookedInterestSubject = "";
	private String negotiateInterestSubject = "";
	
	/**
	 * Returns the backOfficeID.
	 * @return long
	 */
	public long getBackOfficeID() {
		return backOfficeID;
	}

	/**
	 * Returns the currencyID.
	 * @return long
	 */
	public long getCurrencyID() {
		return currencyID;
	}

	/**
	 * Returns the recordID.
	 * @return long
	 */
	public long getRecordID() {
		return recordID;
	}

	/**
	 * Returns the subject.
	 * @return String
	 */
	public String getSubject() {
		return subject;
	}

	/**
	 * Returns the tableName.
	 * @return String
	 */
	public String getTableName() {
		return tableName;
	}

	/**
	 * Sets the backOfficeID.
	 * @param backOfficeID The backOfficeID to set
	 */
	public void setBackOfficeID(long backOfficeID) {
		this.backOfficeID = backOfficeID;
	}

	/**
	 * Sets the currencyID.
	 * @param currencyID The currencyID to set
	 */
	public void setCurrencyID(long currencyID) {
		this.currencyID = currencyID;
	}

	/**
	 * Sets the recordID.
	 * @param recordID The recordID to set
	 */
	public void setRecordID(long recordID) {
		this.recordID = recordID;
	}

	/**
	 * Sets the subject.
	 * @param subject The subject to set
	 */
	public void setSubject(String subject) {
		this.subject = subject;
	}

	/**
	 * Sets the tableName.
	 * @param tableName The tableName to set
	 */
	public void setTableName(String tableName) {
		this.tableName = tableName;
	}

	/**
	 * @return the interestSubjectCode
	 */
	public String getInterestSubject() {
		return interestSubject;
	}

	/**
	 * @param interestSubjectCode the interestSubjectCode to set
	 */
	public void setInterestSubject(String interestSubject) {
		this.interestSubject = interestSubject;
	}

	/**
	 * @return the bookedInterestSubjectCode
	 */
	public String getBookedInterestSubject() {
		return bookedInterestSubject;
	}

	/**
	 * @param bookedInterestSubjectCode the bookedInterestSubjectCode to set
	 */
	public void setBookedInterestSubject(String bookedInterestSubject) {
		this.bookedInterestSubject = bookedInterestSubject;
	}

	/**
	 * @return the negotiateInterestSubjectCode
	 */
	public String getNegotiateInterestSubject() {
		return negotiateInterestSubject;
	}

	/**
	 * @param negotiateInterestSubjectCode the negotiateInterestSubjectCode to set
	 */
	public void setNegotiateInterestSubject(String negotiateInterestSubject) {
		this.negotiateInterestSubject = negotiateInterestSubject;
	}

}
