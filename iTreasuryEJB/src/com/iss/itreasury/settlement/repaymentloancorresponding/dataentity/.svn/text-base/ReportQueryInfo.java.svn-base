package com.iss.itreasury.settlement.repaymentloancorresponding.dataentity;

import java.io.Serializable;
import java.sql.Timestamp;
import java.util.*;

import com.iss.itreasury.settlement.base.*;
import com.iss.itreasury.util.*;

/**
 *
 * @author  pepsi
 * @version
 */
public class ReportQueryInfo extends SettlementBaseDataEntity
{
    private long beginClientId = -1;			//客户号起
    private long endClientId = -1;				//客户号止
    private Timestamp beginDtExecute = null;	//贷款日期起
    private Timestamp endDtExecute = null;		//贷款日期止
	/**
	 * @return Returns the beginClientId.
	 */
	public long getBeginClientId() {
		return beginClientId;
	}
	/**
	 * @param beginClientId The beginClientId to set.
	 */
	public void setBeginClientId(long beginClientId) {
		this.beginClientId = beginClientId;
	}
	/**
	 * @return Returns the beginDtExecute.
	 */
	public Timestamp getBeginDtExecute() {
		return beginDtExecute;
	}
	/**
	 * @param beginDtExecute The beginDtExecute to set.
	 */
	public void setBeginDtExecute(Timestamp beginDtExecute) {
		this.beginDtExecute = beginDtExecute;
	}
	/**
	 * @return Returns the endClientId.
	 */
	public long getEndClientId() {
		return endClientId;
	}
	/**
	 * @param endClientId The endClientId to set.
	 */
	public void setEndClientId(long endClientId) {
		this.endClientId = endClientId;
	}
	/**
	 * @return Returns the endDtExecute.
	 */
	public Timestamp getEndDtExecute() {
		return endDtExecute;
	}
	/**
	 * @param endDtExecute The endDtExecute to set.
	 */
	public void setEndDtExecute(Timestamp endDtExecute) {
		this.endDtExecute = endDtExecute;
	}
}