	 * 根据月份获取该月所在的季度
	 * Method getQuarterByMonth.
	 * @param lMonth
	 * @return String
	 */
	public static String getQuarterByMonth(long lMonth)
	{
		String strQuarter = "";
		switch ((int) lMonth)
		{
			case 1 :
				strQuarter = "一";
				break;
			case 2 :
				strQuarter = "一";
				break;
			case 3 :
				strQuarter = "一";
				break;
			case 4 :
				strQuarter = "二";
				break;
			case 5 :
				strQuarter = "二";
				break;
			case 6 :
				strQuarter = "二";
				break;
			case 7 :
				strQuarter = "三";
				break;
			case 8 :
				strQuarter = "三";
				break;
			case 9 :
				strQuarter = "三";
				break;
