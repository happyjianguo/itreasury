package com.iss.itreasury.settlement.setting.dataentity;

import java.sql.Date;

import com.iss.itreasury.settlement.base.SettlementBaseDataEntity;
import com.iss.itreasury.util.Constant;
/**
	ID	ID
	OFFICEID	办事处
	CURRENCYID	币种
	ISINPUTSIGN	是否必输
	ISVIEW	是否显示
	INPUTUSERID	录入人ID
	INPUTDATE	录入时间
	MODIFYUSERID	修改人ID
	MODIFYDATE	修改时间
	STATUSID	状态
**/
public class NotifySettingInfo extends SettlementBaseDataEntity {
	private long id=-1;
	private long officeId=-1;
	private long currencyId=-1;
	private long isInputSign=Constant.PageElementControl.NOTNEEDINPUT;
	private long isView=Constant.PageElementControl.DISPLAY;
	private long inputUserId;
	private Date inputDate;
	private long modifyUserId;
	private Date modifyDate;
	private long StatusId;
	public long getCurrencyId() {
		return currencyId;
	}
	public void setCurrencyId(long currencyId) {
		this.currencyId = currencyId;
	}
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public Date getInputDate() {
		return inputDate;
	}
	public void setInputDate(Date inputDate) {
		this.inputDate = inputDate;
	}
	public long getInputUserId() {
		return inputUserId;
	}
	public void setInputUserId(long inputUserId) {
		this.inputUserId = inputUserId;
	}
	public long getIsInputSign() {
		return isInputSign;
	}
	public void setIsInputSign(long isInputSign) {
		this.isInputSign = isInputSign;
	}
	public long getIsView() {
		return isView;
	}
	public void setIsView(long isView) {
		this.isView = isView;
	}
	public Date getModifyDate() {
		return modifyDate;
	}
	public void setModifyDate(Date modifyDate) {
		this.modifyDate = modifyDate;
	}
	public long getModifyUserId() {
		return modifyUserId;
	}
	public void setModifyUserId(long modifyUserId) {
		this.modifyUserId = modifyUserId;
	}
	public long getOfficeId() {
		return officeId;
	}
	public void setOfficeId(long officeId) {
		this.officeId = officeId;
	}
	public long getStatusId() {
		return StatusId;
	}
	public void setStatusId(long statusId) {
		StatusId = statusId;
	}
	
}
