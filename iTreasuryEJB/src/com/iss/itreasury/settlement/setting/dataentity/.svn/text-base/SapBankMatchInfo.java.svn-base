package com.iss.itreasury.settlement.setting.dataentity;

import java.io.Serializable;

public class SapBankMatchInfo implements Serializable{
	public long id = -1;
	public long banktype =-1;
	public long bankmath =-1;
	public long status =-1;
	public long officeid=-1;
	public long currencyid=-1;
	public String scode="";
	public String sbankaccoutcode="";
	public String sname="";
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public long getBanktype() {
		return banktype;
	}
	public void setBanktype(long banktype) {
		this.banktype = banktype;
	}
	public long getBankmath() {
		return bankmath;
	}
	public void setBankmath(long bankmath) {
		this.bankmath = bankmath;
	}
	public long getStatus() {
		return status;
	}
	public void setStatus(long status) {
		this.status = status;
	}
	public long getOfficeid() {
		return officeid;
	}
	public void setOfficeid(long officeid) {
		this.officeid = officeid;
	}
	public long getCurrencyid() {
		return currencyid;
	}
	public void setCurrencyid(long currencyid) {
		this.currencyid = currencyid;
	}
	public String getSname() {
		return sname;
	}
	public void setSname(String sname) {
		this.sname = sname;
	}
	public String getSbankaccoutcode() {
		return sbankaccoutcode;
	}
	public void setSbankaccoutcode(String sbankaccoutcode) {
		this.sbankaccoutcode = sbankaccoutcode;
	}
	public String getScode() {
		return scode;
	}
	public void setScode(String scode) {
		this.scode = scode;
	}

}
