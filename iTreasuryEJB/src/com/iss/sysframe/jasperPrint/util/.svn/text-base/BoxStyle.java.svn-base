package com.iss.sysframe.jasperPrint.util;

public class BoxStyle {

    private float penLineWidth=(float) 0.0;
    private float topPenLineWidth=(float)0.0;
    private float bottomPenLineWidth=(float)0.0;
    private float leftPenLineWidth=(float)0.0;
    private float rightPenLineWidth=(float)0.0;
    
	public float getPenLineWidth() {
		return penLineWidth;
	}
	public void setPenLineWidth(float penLineWidth) {
		this.penLineWidth = penLineWidth;
	}
	public float getTopPenLineWidth() {
		return topPenLineWidth;
	}
	public void setTopPenLineWidth(float topPenLineWidth) {
		this.topPenLineWidth = topPenLineWidth;
	}
	public float getBottomPenLineWidth() {
		return bottomPenLineWidth;
	}
	public void setBottomPenLineWidth(float bottomPenLineWidth) {
		this.bottomPenLineWidth = bottomPenLineWidth;
	}
	public float getLeftPenLineWidth() {
		return leftPenLineWidth;
	}
	public void setLeftPenLineWidth(float leftPenLineWidth) {
		this.leftPenLineWidth = leftPenLineWidth;
	}
	public float getRightPenLineWidth() {
		return rightPenLineWidth;
	}
	public void setRightPenLineWidth(float rightPenLineWidth) {
		this.rightPenLineWidth = rightPenLineWidth;
	}
    
    

}
