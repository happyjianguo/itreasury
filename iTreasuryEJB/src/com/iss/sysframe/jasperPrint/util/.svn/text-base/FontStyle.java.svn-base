package com.iss.sysframe.jasperPrint.util;

public class FontStyle {

	private String frontName="ו";
	
	private long frontSize=12;

	public String isBold="false";
	
	public String getFrontName() {
		return frontName;
	}

	public void setFrontName(String frontName) {
		this.frontName = frontName;
	}

	public long getFrontSize() {
		return frontSize;
	}

	public void setFrontSize(long frontSize) {
		this.frontSize = frontSize;
	}

	public String getIsBold() {
		return isBold;
	}

	public void setIsBold(String isBold) {
		this.isBold = isBold;
	}
	
}
