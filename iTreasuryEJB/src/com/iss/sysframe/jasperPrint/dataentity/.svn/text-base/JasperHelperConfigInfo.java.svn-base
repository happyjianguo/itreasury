package com.iss.sysframe.jasperPrint.dataentity;


import com.iss.sysframe.jasperPrint.band.JasperColumnFooter;
import com.iss.sysframe.jasperPrint.band.JasperColumnHeader;
import com.iss.sysframe.jasperPrint.band.JasperDetail;
import com.iss.sysframe.jasperPrint.band.JasperEmptyReport;
import com.iss.sysframe.jasperPrint.band.JasperFieldBand;
import com.iss.sysframe.jasperPrint.band.JasperPageFooter;
import com.iss.sysframe.jasperPrint.band.JasperPageHeader;
import com.iss.sysframe.jasperPrint.band.JasperTitle;

public class JasperHelperConfigInfo {

	private JasperFieldBand fieldBand=new JasperFieldBand();
	
	private JasperTitle title=new JasperTitle();
	
	private JasperColumnHeader colHeader=new JasperColumnHeader();
	
	private JasperColumnFooter colFooter=new JasperColumnFooter();
	
	private JasperDetail detail=new JasperDetail();
	
	private JasperPageHeader pageHeader=new JasperPageHeader();
	
	private JasperPageFooter pageFooter=new JasperPageFooter();
	
	private JasperEmptyReport report=new JasperEmptyReport();
	
	private String outFilePath="";	

	public JasperFieldBand getFieldBand() {
		return fieldBand;
	}

	public void setFieldBand(JasperFieldBand fieldBand) {
		this.fieldBand = fieldBand;
	}

	public JasperTitle getTitle() {
		return title;
	}

	public void setTitle(JasperTitle title) {
		this.title = title;
	}

	public JasperColumnHeader getColHeader() {
		return colHeader;
	}

	public void setColHeader(JasperColumnHeader colHeader) {
		this.colHeader = colHeader;
	}

	public JasperColumnFooter getColFooter() {
		return colFooter;
	}

	public void setColFooter(JasperColumnFooter colFooter) {
		this.colFooter = colFooter;
	}

	public JasperDetail getDetail() {
		return detail;
	}

	public void setDetail(JasperDetail detail) {
		this.detail = detail;
	}

	public String getOutFilePath() {
		return outFilePath;
	}

	public void setOutFilePath(String outFilePath) {
		this.outFilePath = outFilePath;
	}

	public JasperEmptyReport getReport() {
		return report;
	}

	public void setReport(JasperEmptyReport report) {
		this.report = report;
	}

	public JasperPageHeader getPageHeader() {
		return pageHeader;
	}

	public void setPageHeader(JasperPageHeader pageHeader) {
		this.pageHeader = pageHeader;
	}

	public JasperPageFooter getPageFooter() {
		return pageFooter;
	}

	public void setPageFooter(JasperPageFooter pageFooter) {
		this.pageFooter = pageFooter;
	}
	
}
