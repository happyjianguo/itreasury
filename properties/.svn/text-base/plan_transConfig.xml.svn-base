<xmlConfig>
	<trans transName="拆借业务--同业拆入" transId="">
		<!--
		@officeID
		@currencyID 
		@executeDate
		@transDate
		-->
		<strSql>
		select C_mBorrowAmount, NSUBINVESTMENTTYPEID, SSERIALNO
		from sett_transinvestment a 
		where 	C_dtBorrowDate = @transDate
		AND nStatusID != 0
		and NSUBINVESTMENTTYPEID=10
		and nofficeid = @officeID 
		and ncurrencyid = @currencyID

		</strSql>
		   <field>
		      <entityFieldName>ActualAmount</entityFieldName>
		      <databaseFieldName>C_mBorrowAmount</databaseFieldName>  <!-- C_mBorrowAmount -->
		      <isGetDataFromDatabase>true</isGetDataFromDatabase>
		      <defaultValue></defaultValue>
		   </field>
		   
	       <field>
		      <entityFieldName>AccountTypeId</entityFieldName>
		      <databaseFieldName>NSUBINVESTMENTTYPEID</databaseFieldName>
		      <isGetDataFromDatabase>true</isGetDataFromDatabase>
		      <defaultValue></defaultValue>
		   </field>
		   
		   <field>
		      <entityFieldName>AmountDirection</entityFieldName>
		      <databaseFieldName></databaseFieldName>
		      <isGetDataFromDatabase>false</isGetDataFromDatabase>
		      <defaultValue>TPConstant.AmountDirection.PLUS</defaultValue>
		   </field>
		   
		   <field>
		      <entityFieldName>TransactionCode</entityFieldName>
		      <databaseFieldName>SSERIALNO</databaseFieldName>
		      <isGetDataFromDatabase>true</isGetDataFromDatabase>
		      
		   </field>
	</trans>
	
