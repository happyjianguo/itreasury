body,p,td {
	font-size: 12px;
	color: #000000;
}
input,textarea,select {
	font-size:12px;
	color:#000000;
}
a {
	color:#0066CC;
}
a:link ,a:visited,a:hover ,a:active {
	color:#000;
	text-decoration:underline;
}
