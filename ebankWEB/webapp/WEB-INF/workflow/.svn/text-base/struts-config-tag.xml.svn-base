<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "../../dtds/struts-config_1_2.dtd">

<struts-config>

	<form-beans>
		<form-bean name="wfSearchCtrl" type="com.iss.inut.workflow.webmanage.web.WFSearchCtrl" />
 	</form-beans>

	<action-mappings>
		<action path="/wfSearchCtrl" type="com.iss.inut.web.BaseAction" name="wfSearchCtrl"
		 				scope="request" parameter="operate" validate="false">		
			  <forward name="availableStepList" path="/workflow/jsp/wfsearch/AvailableStepList.jsp" />
		</action>

	</action-mappings>

</struts-config>
