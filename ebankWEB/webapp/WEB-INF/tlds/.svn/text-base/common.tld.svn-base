<!-- 放大镜控件 -->
	<tag>
		<name>dic</name>
		<tag-class>com.iss.itreasury.ebank.tags.Dictionary</tag-class>
		<body-content>scriptless</body-content>
		<description>放大镜主体控件</description>
		<attribute>
			<name>id</name>
			<description><![CDATA[文本框对应id,name]]></description>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>size</name>
			<description><![CDATA[文本框宽度]]></description>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>			
		</attribute>
		<attribute>
			<name>form</name>
			<description><![CDATA[表单名]]></description>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>	
		<attribute>
			<name>title</name>
			<description><![CDATA[窗口名称]]></description>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>sqlFunction</name>
			<description><![CDATA[查询函数名]]></description>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>	
		<attribute>
			<name>sqlParams</name>
			<description><![CDATA[查询参数]]></description>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>	
		<attribute>
			<name>nextFocus</name>
			<description><![CDATA[下一个焦点]]></description>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>		
		<attribute>
			<name>width</name>
			<description><![CDATA[放大镜宽度]]></description>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>			
		</attribute>	
		<attribute>
			<name>value</name>
			<description><![CDATA[放大镜初始值]]></description>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>			
		</attribute>	
		<attribute>
			<name>properties</name>
			<description><![CDATA[属性]]></description>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>				
		</attribute>			
		<attribute>
			<name>type</name>
			<description><![CDATA[输入框类型 "text"或者"textarea" 默认为"text"]]></description>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>		
		<attribute>
			<name>row</name>
			<description><![CDATA[文本域行数  输入框类型为"textarea"有效]]></description>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>
		<attribute>
			<name>col</name>
			<description><![CDATA[文本域列数 输入框类型为"textarea"有效]]></description>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>	
		<attribute>
			<name>needRemind</name>
			<description><![CDATA[文本域是否需要提醒 输入框类型为"textarea"有效 默认为false]]></description>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>	
		<attribute>
			<name>maxlength</name>
			<description><![CDATA[文本域提醒最大字数 输入框类型为"textarea"且需要提醒时有效]]></description>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>
		<attribute>
			<name>position</name>
			<description><![CDATA[弹出层相对位置 "bottom"或者"top" 默认为"bottom"]]></description>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>
	</tag>
	
